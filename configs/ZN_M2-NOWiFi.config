CONFIG_PACKAGE_kmod-nft-offload=y   # 流量卸载加速，将已建立连接交给硬件/快速路径处理
CONFIG_PACKAGE_kmod-nft-queue=y     # 数据包队列，将包送往用户态程序处理后再转发
CONFIG_PACKAGE_kmod-nft-socket=y    # Socket 匹配，透明代理识别本地进程连接必需
CONFIG_PACKAGE_kmod-nft-xfrm=y     # IPsec/XFRM 策略匹配，用于 VPN 流量的规则过滤
CONFIG_PACKAGE_kmod-sched-core=y              # 流量调度核心
