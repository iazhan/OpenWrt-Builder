# CONFIG_BUSYBOX_CONFIG_IP is not set           # 禁用 BusyBox 内置的精简版 ip 命令
# ============================================
# 系统工具
# ============================================
CONFIG_PACKAGE_ca-certificates=y              # CA 证书
CONFIG_PACKAGE_cgi-io=y                       # CGI I/O 工具
CONFIG_PACKAGE_default-settings=y             # 默认设置
CONFIG_PACKAGE_default-settings-chn=y         # 中国区默认设置
CONFIG_PACKAGE_htop=y                         # 系统监控工具
CONFIG_PACKAGE_terminfo=y                     # 终端信息数据库
CONFIG_PACKAGE_ttyd=y                         # Web 终端守护进程
CONFIG_PACKAGE_resolveip=y                    # IP 解析工具

# ============================================
# RPC 与包管理
# ============================================
CONFIG_USE_APK=y                                    # 启用 APK 包管理器
CONFIG_PACKAGE_apk-mbedtls=y                     # 启用 APK (mbedTLS)
CONFIG_PACKAGE_rpcd=y                     # RPC 守护进程
CONFIG_PACKAGE_rpcd-mod-file=y                # 文件操作模块
CONFIG_PACKAGE_rpcd-mod-iwinfo=y              # 无线信息模块
CONFIG_PACKAGE_rpcd-mod-luci=y                # LuCI 模块
CONFIG_PACKAGE_rpcd-mod-rrdns=y               # 反向 DNS 模块
CONFIG_PACKAGE_rpcd-mod-ucode=y               # ucode 模块

# ============================================
# Web 服务器
# ============================================
CONFIG_PACKAGE_uhttpd=y                       # uHTTPd Web 服务器
CONFIG_PACKAGE_uhttpd-mod-ubus=y              # ubus 集成模块

# ============================================
# ucode 模块
# ============================================
CONFIG_PACKAGE_ucode-mod-html=y               # HTML 模块
CONFIG_PACKAGE_ucode-mod-lua=y                # Lua 模块
CONFIG_PACKAGE_ucode-mod-math=y               # 数学模块
# CONFIG_PACKAGE_ucode-mod-digest is not set    # 禁用摘要模块
# CONFIG_PACKAGE_ucode-mod-nl80211 is not set   # 禁用无线模块
# CONFIG_PACKAGE_ucode-mod-rtnl is not set      # 禁用路由模块
# CONFIG_PACKAGE_ucode-mod-uloop is not set     # 禁用 uloop 模块

# ============================================
# 其他禁用的包
# ============================================
# CONFIG_FORTIFY_SOURCE is not set
# CONFIG_PACKAGE_opkg is not set                    # 禁用 OPKG 包管理器
# CONFIG_OPKG_USE_CURL is not set                   # 禁用 OPKG 使用 CURL
# CONFIG_PACKAGE_apk-openssl is not set          # 禁用 APK (OpenSSL)
# CONFIG_PACKAGE_kmod-hwmon-core is not set      # 禁用硬件监控
# CONFIG_PACKAGE_kmod-i2c-core is not set        # 禁用 I2C
# CONFIG_PACKAGE_kmod-mhi-bus is not set         # 禁用 MHI 总线
# CONFIG_PACKAGE_kmod-qrtr-mhi is not set        # 禁用 QRTR MHI
# CONFIG_PACKAGE_kmod-qrtr-smd is not set        # 禁用 QRTR SMD
# CONFIG_PACKAGE_libopenssl-conf is not set      # 禁用 OpenSSL 配置
# CONFIG_PACKAGE_libopenssl-legacy is not set    # 禁用 OpenSSL 旧版

# ============================================
# mbedTLS 加密库配置
# ============================================
CONFIG_MBEDTLS_AES_C=y                                              # AES 加密算法，最基础的对称加密
# CONFIG_MBEDTLS_ARIA_C is not set                                  # ARIA 加密（韩国标准，不需要）
# CONFIG_MBEDTLS_CAMELLIA_C is not set                              # Camellia 加密（日本标准，不需要）
# CONFIG_MBEDTLS_CCM_C is not set                                   # CCM 认证加密模式（不需要）
# CONFIG_MBEDTLS_CIPHER_MODE_OFB is not set                         # OFB 流加密模式（不需要）
# CONFIG_MBEDTLS_CIPHER_MODE_XTS is not set                         # XTS 磁盘加密模式（不需要）
CONFIG_MBEDTLS_CMAC_C=y                                             # CMAC 消息认证码
# CONFIG_MBEDTLS_DEBUG_C is not set                                  # 调试输出（生产环境不需要）
# CONFIG_MBEDTLS_DEPRECATED_WARNING is not set                       # 弃用 API 警告（不需要）
CONFIG_MBEDTLS_DES_C=y                                              # DES/3DES，兼容老旧设备需要

# 椭圆曲线配置
# CONFIG_MBEDTLS_ECP_DP_BP256R1_ENABLED is not set                  # Brainpool 曲线（欧洲标准，不需要）
# CONFIG_MBEDTLS_ECP_DP_BP384R1_ENABLED is not set
# CONFIG_MBEDTLS_ECP_DP_BP512R1_ENABLED is not set
CONFIG_MBEDTLS_ECP_DP_CURVE25519_ENABLED=y                          # Curve25519，WireGuard/现代代理协议必需
# CONFIG_MBEDTLS_ECP_DP_CURVE448_ENABLED is not set                 # Curve448（更高强度，不需要）
# CONFIG_MBEDTLS_ECP_DP_SECP192K1_ENABLED is not set                # 老旧短曲线（不需要）
# CONFIG_MBEDTLS_ECP_DP_SECP192R1_ENABLED is not set
# CONFIG_MBEDTLS_ECP_DP_SECP224K1_ENABLED is not set
# CONFIG_MBEDTLS_ECP_DP_SECP224R1_ENABLED is not set
CONFIG_MBEDTLS_ECP_DP_SECP256K1_ENABLED=y                          # secp256k1，部分代理协议需要
CONFIG_MBEDTLS_ECP_DP_SECP256R1_ENABLED=y                          # secp256r1 (P-256)，TLS 最常用曲线
CONFIG_MBEDTLS_ECP_DP_SECP384R1_ENABLED=y                          # secp384r1 (P-384)，部分证书使用
CONFIG_MBEDTLS_ECP_DP_SECP521R1_ENABLED=y                          # secp521r1 (P-521)，最高强度曲线

CONFIG_MBEDTLS_ENTROPY_FORCE_SHA256=y                               # 强制使用 SHA256 作为随机熵源
CONFIG_MBEDTLS_GCM_C=y                                              # GCM 认证加密，TLS 1.3 必需
CONFIG_MBEDTLS_HKDF_C=y                                             # HKDF 密钥派生，TLS 1.3 必需

# 密钥交换配置
# CONFIG_MBEDTLS_KEY_EXCHANGE_DHE_PSK_ENABLED is not set            # DHE-PSK（老旧，不需要）
# CONFIG_MBEDTLS_KEY_EXCHANGE_DHE_RSA_ENABLED is not set            # DHE-RSA（老旧，不需要）
CONFIG_MBEDTLS_KEY_EXCHANGE_ECDHE_ECDSA_ENABLED=y                  # ECDHE-ECDSA，现代 TLS 主流密钥交换
CONFIG_MBEDTLS_KEY_EXCHANGE_ECDHE_PSK_ENABLED=y                    # ECDHE-PSK
CONFIG_MBEDTLS_KEY_EXCHANGE_ECDHE_RSA_ENABLED=y                    # ECDHE-RSA，兼容 RSA 证书
# CONFIG_MBEDTLS_KEY_EXCHANGE_ECDH_ECDSA_ENABLED is not set         # 静态 ECDH（无前向保密，不需要）
# CONFIG_MBEDTLS_KEY_EXCHANGE_ECDH_RSA_ENABLED is not set
CONFIG_MBEDTLS_KEY_EXCHANGE_PSK_ENABLED=y                           # PSK 预共享密钥
# CONFIG_MBEDTLS_KEY_EXCHANGE_RSA_ENABLED is not set                 # 纯 RSA 密钥交换（无前向保密，不需要）
# CONFIG_MBEDTLS_KEY_EXCHANGE_RSA_PSK_ENABLED is not set

CONFIG_MBEDTLS_NIST_KW_C=y                                          # NIST 密钥封装
# CONFIG_MBEDTLS_PLATFORM_C is not set                               # 平台抽象层（不需要）
CONFIG_MBEDTLS_PSA_CRYPTO_CLIENT=y                                   # PSA 加密 API，新版 mbedTLS 标准接口
# CONFIG_MBEDTLS_RIPEMD160_C is not set                              # RIPEMD-160 哈希（不需要）
CONFIG_MBEDTLS_RSA_NO_CRT=y                                         # RSA 不使用 CRT 优化，节省内存
# CONFIG_MBEDTLS_SELF_TEST is not set                                # 自检测试（生产环境不需要）

# TLS 协议版本
CONFIG_MBEDTLS_SSL_PROTO_TLS1_2=y                                   # TLS 1.2，当前主流版本
CONFIG_MBEDTLS_SSL_PROTO_TLS1_3=y                                   # TLS 1.3，最新版本，sing-box/nikki 推荐
# CONFIG_MBEDTLS_SSL_RENEGOTIATION is not set                        # TLS 重协商（有安全风险，不需要）
CONFIG_MBEDTLS_SSL_TLS1_3_COMPATIBILITY_MODE=y                      # TLS 1.3 兼容模式，兼容旧客户端
CONFIG_MBEDTLS_SSL_TLS1_3_KEY_EXCHANGE_MODE_EPHEMERAL_ENABLED=y    # TLS 1.3 临时密钥交换（前向保密）
CONFIG_MBEDTLS_SSL_TLS1_3_KEY_EXCHANGE_MODE_PSK_ENABLED=y          # TLS 1.3 PSK 模式
CONFIG_MBEDTLS_SSL_TLS1_3_KEY_EXCHANGE_MODE_PSK_EPHEMERAL_ENABLED=y # TLS 1.3 PSK+临时密钥混合模式

# 多线程支持
CONFIG_MBEDTLS_THREADING_C=y                                        # 多线程抽象层
CONFIG_MBEDTLS_THREADING_PTHREAD=y                                   # 使用 POSIX 线程实现
# CONFIG_MBEDTLS_VERSION_C is not set                                # 版本查询 API（不需要）
# CONFIG_MBEDTLS_VERSION_FEATURES is not set                         # 特性查询 API（不需要）